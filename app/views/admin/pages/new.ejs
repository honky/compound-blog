<ul class="breadcrumb">
    <li><a href="/">Home</a> <span class="divider">/</span></li>
    <li><a href="<%- pathTo.admin_pages() %>">pages</a> <span class="divider">/</span></li>
    <li class="active"><a href="#">New page</a></li>
</ul>

<div class="page-header">
    <h1>New page</h1>
</div>

<% var form = formFor(page, {action: pathTo.admin_pages(), method: 'POST', id: "page-form", class: 'form-horizontal'}) %>
<%- form.begin() %>
<%- include _form %>
<div class="form-actions">
    <%- form.submit('<i class="icon-ok icon-white"></i> Create page', {class: 'btn btn-primary'}) %> or
    <%- linkTo('Cancel', pathTo.admin_pages(), {class: 'btn'}) %>
</div>
<%- form.end() %>
