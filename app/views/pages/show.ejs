<ul class="breadcrumb">
    <li><a href="/"><%= t('locale.home') %></a> <span class="divider">/</span></li>
    <li><a href="<%- pathTo.pages %>"><%= t('pages_controller_client.pages') %></a> <span class="divider">/</span></li>
    <li class="active"><a href="#"><%= t('pages_controller_client.action_show_title') %></a></li>
</ul>

<div class="page-header">
    <h1><%= t('pages_controller_client.action_show_title') %></h1>
</div>

<table class="table table-bordered">
    <tbody>
        <tr><th>page_id</th><td><%= page.page_id %></td></tr>

        <tr><th>title</th><td><%= page.title %></td></tr>

        <tr><th>teaser</th><td><%= page.teaser %></td></tr>

        <tr><th>content</th><td><%- page.content_formated() %></td></tr>

        <tr><th>language</th><td><%= page.language %></td></tr>

        <tr><th>created</th><td><%= page.created %></td></tr>

        <tr><th>createdBy</th><td><%= page.createdBy %></td></tr>

        <tr><th>modified</th><td><%= page.modified %></td></tr>

        <tr><th>modifiedBy</th><td><%= page.modifiedBy %></td></tr>

        <tr><th>isPublic</th><td><%= page.isPublic %></td></tr>

        <tr><th>isRegisteredOnly</th><td><%= page.isRegisteredOnly %></td></tr>
        <tr><th>Tags</th><td><pre><%- JSON.stringify(page.tags_fetched) %></pre>
        <tr><th>Categories</th><td><pre><%= JSON.stringify(page.categories_fetched) %></pre></td></tr>
        <tr><th>Comments</th><td><pre><%= JSON.stringify(page.comments_fetched) %></pre><br/>
        <a href="<%- pathTo.page_comments(page) %>">show comments of this page </a> </td></tr>
    </tbody>
</table>

